<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table>
        <tr>
            <th>Title</th>
            <th>Price</th>
            <th>Photo</th>
        </tr>
        <% product.forEach(function(item){ %>
            <tr>
                <th><%=item.title%></th>
                <th><%=item.price%></th>
                <th><img src=<%=item.thumbnail %> ></img></th>
            </tr>
        <% }); %>       
        
    </table>
</body>
</html>




    